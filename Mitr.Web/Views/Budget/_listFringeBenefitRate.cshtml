@model Mitr.Models.BudgetMaster.FringeBenefitRate

<table class="table">
    <thead>
        <tr>
            <th>Category</th>
            <th class="w-80">Rate</th>
        </tr>
    </thead>
    <tbody>
        @{
            string MenuID = "0";
            if (ViewBag.MenuID != null)
            {
                MenuID = ViewBag.MenuID;
            }



        }
        @if (Model.listAddFringeBenefitRate.Count > 0)
        {
            for (int i = 0; i < Model.listAddFringeBenefitRate.Count; i++)
            {
                <tr>
                    <td>
                        <strong>
                            @Model.listAddFringeBenefitRate[i].Category
                            @Html.HiddenFor(model => model.listAddFringeBenefitRate[i].Category)
                        </strong>
                    </td>
                    <td>
                        @Html.TextBoxFor(model => model.listAddFringeBenefitRate[i].Rate, new { @class = "form-control", @onkeypress = "return isNumberKey(event,this)" })
                      
                        @Html.HiddenFor(model => model.listAddFringeBenefitRate[i].Id)
                    </td>

                </tr>

              
            }
        }
    </tbody>
</table>
