												}
												else
												{
													<button type="button" class="btn mtr-g-grn isDisabled" disabled data-toggle="tooltip" title="Your previous request is pending on HR, please obtain approval from HR" id="btnupdatedin"><i class="fas fa-edit"></i>Update</button>
												}


											</td>
											break;
										case "uan":
											<td class="w-200 ">
												<div class="form-group mb-0">
													<label class="lbl-ab">UAN</label>
													@Html.TextBoxFor(model => Model.EMPAttachmentsList[i].No, new { @class = "form-control" })
													@Html.ValidationMessageFor(model => Model.EMPAttachmentsList[i].No, "", new { @class = "text-danger" })
												</div>
											</td>
											<td class="w-200 ">
												<div class="form-group mb-0">
													<label class="lbl-ab">Name on UAN</label>
													@Html.TextBoxFor(model => Model.EMPAttachmentsList[i].Name, new { @class = "form-control" })
													@Html.ValidationMessageFor(model => Model.EMPAttachmentsList[i].Name, "", new { @class = "text-danger" })
												</div>
											</td>
											break;
										case "old pf":
											<td class="w-200 ">
												<div class="form-group mb-0">
													<label class="lbl-ab">Old PF No</label>
													@Html.TextBoxFor(model => Model.EMPAttachmentsList[i].No, new { @class = "form-control", @placeholder = "Enter" })
													@Html.ValidationMessageFor(model => Model.EMPAttachmentsList[i].No, "", new { @class = "text-danger" })
												</div>
											</td>
											@*<td class="w-200 ">

														<button type="button" class="btn m-0 mtr-g-grn clc-35 mt-13" data-toggle="tooltip" title="Update" id="btnupdateOPF" onclick="UpdateOPF_Update(this)"><i class="fas fa-edit m-0"></i></button>

												</td>*@
											break;
										case "pio/oci":
											<td class="w-200 ">
												<div class="form-group mb-0">
													<label class="lbl-ab">PIO/OCI</label>
													@Html.TextBoxFor(model => Model.EMPAttachmentsList[i].No, new { @class = "form-control", @placeholder = "Enter" })
													@Html.ValidationMessageFor(model => Model.EMPAttachmentsList[i].No, "", new { @class = "text-danger" })
												</div>
											</td>
											<td class="w-200 ">
												<div class="form-group mb-0">
													<label class="lbl-ab">Name on PIO/OCI</label>
													@Html.TextBoxFor(model => Model.EMPAttachmentsList[i].Name, new { @class = "form-control", @placeholder = "Enter" })
													@Html.ValidationMessageFor(model => Model.EMPAttachmentsList[i].Name, "", new { @class = "text-danger" })
												</div>
											</td>
											@*<td class="w-200 ">

														<button type="button" class="btn m-0 mtr-g-grn clc-35 mt-13" data-toggle="tooltip" title="Update"  id="btnupdatePIO" onclick="UpdatePIO_Update(this)"><i class="fas fa-edit m-0"></i></button>

												</td>*@
											break;

										case "driving license":
											<td class="w-200 ">
												<div class="form-group mb-0">
                                                    <label class="lbl-ab">Driving License No</label>
													@Html.TextBoxFor(model => Model.EMPAttachmentsList[i].No, new { @class = "form-control" })
													@Html.ValidationMessageFor(model => Model.EMPAttachmentsList[i].No, "", new { @class = "text-danger" })
												</div>
											</td>
											break;
										case "professional tax already deposited":
											<td class="w-200 ">
												<div class="form-group mb-0">
													<label class="lbl-ab">Amount</label>
													@Html.TextBoxFor(model => Model.EMPAttachmentsList[i].No, new { @class = "form-control", @placeholder = "Enter" })
													@Html.ValidationMessageFor(model => Model.EMPAttachmentsList[i].No, "", new { @class = "text-danger" })
												</div>
											</td>
											<td class="w-150">
												<div class="form-group mb-0">
													<label class="lbl-ab">Remarks</label>
													@Html.TextAreaFor(model => Model.EMPAttachmentsList[i].Remarks, new { @class = "form-control", @placeholder = "Enter" })
													@Html.ValidationMessageFor(model => Model.EMPAttachmentsList[i].Remarks, "", new { @class = "text-danger" })
												</div>
											</td>
											@*<td class="w-200 ">
														<button type="button" class="btn m-0 mtr-g-grn clc-35 mt-13" data-toggle="tooltip" title="Update"  id="btnupdateprofessional" onclick="Updateprofessional_Update(this)"><i class="fas fa-edit m-0"></i></button>

												</td>*@
											break;

										default:

											<td class="w-200 ">
												<div class="form-group mb-0">
													<label class="lbl-ab">No</label>
													@Html.TextBoxFor(model => Model.EMPAttachmentsList[i].No, new { @class = "form-control" })
													@Html.ValidationMessageFor(model => Model.EMPAttachmentsList[i].No, "", new { @class = "text-danger" })
													<input type="hidden" value="@Model.EMPAttachmentsList[i].No" id="hdfEMPAttachmentsListDlno" />
												</div>
											</td>
											<td class="w-200">
												<div class="form-group mb-0">
													<label class="lbl-ab">Name</label>
													@Html.TextBoxFor(model => Model.EMPAttachmentsList[i].Name, new { @class = "form-control" })
													@Html.ValidationMessageFor(model => Model.EMPAttachmentsList[i].Name, "", new { @class = "text-danger" })
													<input type="hidden" value="@Model.EMPAttachmentsList[i].Name" id="hdfEMPAttachmentsListDlName" />
												</div>
											</td>
											<td class="w-100">
												<div class="form-group mb-0">
													<label class="lbl-ab">Issue Date</label>
													@Html.TextBoxFor(model => Model.EMPAttachmentsList[i].IssueDate, new { @class = "form-control", @type = "date" })
													@Html.ValidationMessageFor(model => Model.EMPAttachmentsList[i].IssueDate, "", new { @class = "text-danger" })
													<input type="hidden" value="@Model.EMPAttachmentsList[i].IssueDate" id="hdfEMPAttachmentsListDlIssueDate" />
												</div>
											</td>
											<td class="w-100">
												<div class="form-group mb-0">
													<label class="lbl-ab">Expiry Date</label>
													@Html.TextBoxFor(model => Model.EMPAttachmentsList[i].ExpiryDate, new { @class = "form-control", @type = "date" })
													@Html.ValidationMessageFor(model => Model.EMPAttachmentsList[i].ExpiryDate, "", new { @class = "text-danger" })
													<input type="hidden" value="@Model.EMPAttachmentsList[i].ExpiryDate" id="hdfEMPAttachmentsListDlExpiryDate" />
												</div>
											</td>
											<td class="w-150">
												<div class="form-group mb-0">
													<label class="lbl-ab">Place Of Issue</label>
													@Html.TextBoxFor(model => Model.EMPAttachmentsList[i].PlaceOfIssue, new { @class = "form-control" })
													@Html.ValidationMessageFor(model => Model.EMPAttachmentsList[i].PlaceOfIssue, "", new { @class = "text-danger" })
													<input type="hidden" value="@Model.EMPAttachmentsList[i].PlaceOfIssue" id="hdfEMPAttachmentsListDlPlaceOfIssue" />
												</div>
											</td>
											<td class="w-300" colspan="2">
												<div class="form-group mb-0">
													<label class="lbl-ab">Remark</label>
