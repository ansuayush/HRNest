												{
													<button type="button" class="btn m-0 mtr-g-grn clc-35 mt-13" data-toggle="tooltip" title="Update" id="btnupdatePassport" onclick="Updatepassport_Update(this)"><i class="fas fa-edit m-0"></i></button>
												}
												else
												{
													<button type="button" class="btn mtr-g-grn isDisabled" disabled data-toggle="tooltip" title="Your previous request is pending on HR, please obtain approval from HR" id="btnupdatePassport"><i class="fas fa-edit"></i>Update</button>
												}


											</td>
											break;
										case "director identification number":
											<td class="w-200 ">
												<div class="form-group mb-0">
													<label class="lbl-ab">DIN</label>
													@Html.TextBoxFor(model => Model.EMPAttachmentsList[i].No, new { @class = "form-control" })
													@Html.ValidationMessageFor(model => Model.EMPAttachmentsList[i].No, "", new { @class = "text-danger" })
													<input type="hidden" value="@Model.EMPAttachmentsList[i].No" id="hdfEMPAttachmentsListDin" />
												</div>
											</td>
											<td class="w-200 ">
												<div class="form-group mb-0">
													<label class="lbl-ab">Name on DIN</label>
													@Html.TextBoxFor(model => Model.EMPAttachmentsList[i].Name, new { @class = "form-control" })
													@Html.ValidationMessageFor(model => Model.EMPAttachmentsList[i].Name, "", new { @class = "text-danger" })
													<input type="hidden" value="@Model.EMPAttachmentsList[i].Name" id="hdfEMPAttachmentsListNameOfDin" />
												</div>
											</td>
