                            <li>
                                <label>Department</label><span id="labelsdept"></span>
                            </li>
                            <li>
                                <label>Designation</label><span id="labelsdesg"></span>
                            </li>
                            <li>  <label>Requested Date</label><span id="labelsreqdate"></span></li>
                            <li><label>Last Working Day </label><span id="labelsRdate"></span> </li>
                            
                        </ul>

                    </div>
                    <div class="col-sm-12">
                        <div class="table-responsive">
                            <table id="tabelHeadsaveFinance" class="table mt-0  table-bordered vt-a dt-responsive nowrap tbltick new_width" data-select2-id="datatable">
                                <thead>
                                    <tr>
                                        <th width="30">S.No</th>
                                        <th width="100">Department</th>
                                        <th width="200">Asset ID</th>
                                        <th>Date of Return</th>
                                        <th>Description</th>
                                        <th>Impact</th>
                                        <th>Status</th>
                                        <th width="150">Remarks</th>
                                        <th width="100">Recovered</th>
                                        <th width="100">Amount of Recovery</th>
