                                        <th>Component </th>
                                        <th width="150">Head Finance Remarks</th>
                                    </tr>
                                </thead>
                                <tbody>
                                </tbody>
                            </table>
                        </div>
                    </div>



                </div>
            </div>
            <!-- Modal footer -->
            <div class="p-0 text-center ">

                <button type="button" class="btn blk-gdn-btn btn-l " id="btnSendHR" style="display:none" onclick="ProceedFinancedata()"><i class="fas fa-arrow-circle-right"></i>Proceed &amp; Send to HR</button>
                <button type="button" class="btn mtr-o-grn btn-l " id="btnSaveHR" style="display:none" onclick="SaveBandBFinancedata()"><i class="fas fa-save"></i>Save</button>
                <button type="button" class="btn cnl-btn ripplelink" data-dismiss="modal"><i class="fas fa-times"></i>Close</button>
            </div>
        </div>
    </div>
</div>

@section scripts    {
    @System.Web.Optimization.Scripts.Render("~/bundle/unobtrusivejs")
    @System.Web.Optimization.Scripts.Render("~/bundle/dataTablesjs")
    <script src="~/assets/design/Scripts/AddInit.js?v=@clsApplicationSetting.GetWebConfigValue("Version")"></script>
    <script src="~/assets/design/scripts/DevScriptExit/ExitHeadFinNoc.js"></script>
    <script>



    </script>


