@model IList<ProjectMappedList>


<div class="table-responsive">
	<table class="table table-scroll act-table">
		<thead>
			<tr>
				<th class="w-50">S No</th>
				<th>Task</th>
				<th class="w-50 text-center">Action</th>
			</tr>
		</thead>
		<tbody>
			@if (Model.Count > 0)
			{
				int count = 0;
				foreach (var item in Model)
				{
					count++;
					<tr>
						<td>
							@count
							<input type="hidden" name="ST" value="@item.ID" />
						</td>
						<td>@item.Description</td>

						<td  class="text-center">
							<a onclick="DeleteTaskRow(this)" class="remove" data-toggle="tooltip" data-original-title="Remove">
								<i class="fas fa-window-close red-clr" aria-hidden="true"></i>
							</a>
						</td>

					</tr>
				}
			}
		</tbody>
	</table>

</div>