@model Mitr.Models.TravelExpenseCompleteRequest
@{
	Layout = null;
	string name = Model.TravelExpense.TravellerName;
	string[] EmployeeCode = name.Split(new Char[] { ' ', '(', ')', '-', '\n', '\t' });
	string MenuID = ViewBag.MenuID.ToString();
}
<!DOCTYPE html>


<html>

<head>
	<meta charset="utf-8" />
	<meta name="viewport" content="width=device-width, initial-scale=1.0">

	<title>Travel Expense Report</title>
	<meta name="title" content="Mitr: ConsolidateActivityLog" />
	<meta name="keywords" content="Mitr: ConsolidateActivityLog" />
	<meta name="description" content="Mitr: ConsolidateActivityLog" />

	<link href="https://fonts.googleapis.com/css?family=Roboto:300,400,700&display=swap" rel="stylesheet">

	<link rel="shortcut icon" href="/assets/design/images/favicon.png" type="image/x-icon">
	<link rel="icon" href="~/assets/design/images/favicon.png" type="image/png" sizes="50x50">
	<link href='~/assets/design/css/icons.css?v=1.6' rel='stylesheet' />
	<link href='~/assets/design/css/bootstrap.min.css?v=1.6' rel='stylesheet' />
	<link href='~/assets/design/css/style.css?v=1.6' rel='stylesheet' />
	<link href='~/assets/design/css/custom.css?v=1.6' rel='stylesheet' />
	<link href='~/assets/design/css/responsive.css?v=1.6' rel='stylesheet' />
	<link href="~/assets/design/css/modelprint.css?v=1.6" rel="stylesheet" type="text/css" media="print">


</head>
<body class=" header">
	<div class="print-header">
		<div class="container">
			<div class="row">
				<div class="col-md-12 col-sm-12 mx-auto text-right">
					<a class="btn blk-gdn-btn" data-toggle="modal" data-target="#ftr"><i class="far fa-clipboard"></i> Fill Travel Report</a>

					<a onclick="window.print()" class="btn mtr-g-grn btn-l ripplelink printclick ">
						<i class="fa fa-print"></i>Print
					</a>
					@*<a href="../view-reimbursement-book.html" class="btn cnl-btn ripplelink ">
							<i class="fas fa-times"></i>Close
						</a>*@
					@if (ViewBag.Status == "TravelExpense")
					{
						@Html.CreateAnchor(MenuID, "/Travel/PaymentStatusToStaff", "Close", "btn cnl-btn btn-l ripplelink", "Close", "fas fa-arrow-left", "")
					}
					else
					{
						@*<button type="button" name="Command" value="Close" id="btnclose" class="btn mtr-g-grn btn-l ripplelink">
								<i class="fa fa-check"></i>Close
							</button>
							@Html.HiddenFor(model => model.TravelExpense.ReqNo)*@
						@Html.CreateAnchor(MenuID, "/Travel/TravelDashboard", "Back", "btn cnl-btn btn-l ripplelink", "Close", "fas fa-arrow-left", "")

					}

				</div>
			</div>
		</div>
	</div>
	<div class="container mt-5 pt-5">
		<div class="row">
			<div class="col-sm-12 mx-auto">
				<div class="inner-main">

					<div class="bgbox">
						<div class="row">
							<div class="col-sm-2 mb-0 align-self-center">
								<img src="https://mitr2.c3india.org/assets/design/images/c3.png" alt="">
							</div>
							<div class="col-sm-8 mb-0 text-center align-self-center">
								<strong class="cntitle">Centre for Catalyzing Change</strong><br>
								<small class="fn-md">C-27, 2nd Floor Qutab Institutional Area, New Delhi - 110 016</small>
							</div>

							<div class="col-sm-2 mb-0 align-self-center text-right">
								<img src="https://mitr2.c3india.org/assets/design/images/mitr-logo.png" alt="">
							</div>
						</div>
					</div>
					<div class="inner-main pb-0">
						<div class="row">
							<div class="col-md-12 form-group">
								<h2 class="heading-one mb-0">Travel Expense Report</h2>
							</div>
							<div class="col-sm-4 form-group">
								<label>Employee Name </label>
								@*<p>@Model.TravelExpense.TravellerName</p>*@
								<p>@EmployeeCode[0] @EmployeeCode[1]</p>
								@Html.HiddenFor(model => model.TravelExpense.emp_id)
								@Html.HiddenFor(model => model.TravelExpense.ReqNo)
							</div>

							<div class="col-sm-4 form-group">
								<label>Employee Code </label>
								@*<p>@EmployeeCode[3]</p>*@
								<p>@Model.TravelExpense.emp_code</p>
							</div>
							<input type="hidden" value="@ViewBag.projectcout" id="hdfprojectcount" />
							<div class="col-sm-4 form-group">
								<label>Supervisor Name</label>
								<p>@Model.TravelExpense.HodName</p>
							</div>
							<div class="col-sm-4 form-group">
								<label>TR No </label>
								<p>@Model.TravelExpense.ReqNo</p>
							</div>

							<div class="col-sm-4 form-group">
								<label>TR Submission Date</label>
								@{
									string TerSubdate = Convert.ToDateTime(Model.TravelExpense.req_date).ToString("dd/MM/yyyy");
								}
								<p>@TerSubdate</p>
							</div>

							<div class="col-sm-4 form-group">
								<label>TR Approval Date</label>
								<p>@Model.TravelExpense.TrApproveDate</p>
							</div>
							<div class="col-sm-4 form-group">
								<label>TER No</label>
								<p>@Model.TravelExpense.TRPN</p>
							</div>


							<div class="col-sm-4 form-group">
								<label>TER Submission Date</label>
								<p>@Model.TravelExpense.TerSubmitionDate</p>
							</div>

							<div class="col-sm-4 form-group">
								<label>TER Approval Date</label>
								@if (Model.TravelExpense.TerApproveDate == "01/01/1900")
								{
									<p></p>
								}
								else
								{
									<p>@Model.TravelExpense.TerApproveDate</p>
								}

							</div>

							<div class="col-sm-12 form-group">
								<label>Purpose of Travel</label>
								<p>@Model.TravelExpense.purpofvisit</p>
							</div>

							<div class="col-md-12 form-group">
								<hr>
								<span class="hr-aps"><i class="fas fa-info-circle"></i>Project Details</span>
							</div>

							<div class="col-md-12 form-group">
								<div class="table-responsive">
									<table id="tblvoucher" class="table v-top table-stripted lg-table mb-0 mt-0">
										<thead>
											<tr>
												<th width="10">SNo. </th>
												<th width="150">Project Name</th>
												<th>Cost Centre</th>

											</tr>
										</thead>
										<tbody>
											@{ int ProjectCount = 0;}
											@foreach (var item in Model.ProjectList)
											{
												ProjectCount++;
												<tr>
													<td>@ProjectCount</td>
													<td>@item.Proj_name</td>
													<td> @item.costcenter_Name </td>
												</tr>
											}

										</tbody>
									</table>
								</div>
							</div>

							<div class="col-md-12 form-group">
								<h2 class="heading-one mb-0">A- Travel Fare</h2>

								<div class="table-responsive">
									<table id="tblvoucher" class="table v-top table-stripted lg-table mb-0 mt-0">
										<thead>
											<tr>
												<th width="10">SNo. </th>
												<th width="100">Date of Depature</th>
												<th>From</th>
												<th>To</th>
												<th>Ticket Source</th>
												<th>Mode of Source</th>
												<th>Boarding Pass/Ticket</th>
												<th width="110"><i class="fas fa-rupee-sign "></i>Amount (Rs.)</th>

											</tr>
										</thead>
										<tbody>
											@{ int Countfare = 0;}
											@foreach (var item in Model.ATRAVELFARE)
											{
												Countfare++;
												DateTime dateTime = Convert.ToDateTime(item.Date);
												<tr>
													<td>@Countfare</td>
													<td>@dateTime.ToString("dd/MM/yyyy")</td>
													<td>@item.FromCity</td>
													<td>@item.ToCity</td>
													<td>@item.TravelMode</td>
													<td>@item.TravelSource</td>
													<td>@item.ticket_bordcast</td>
													<td class="text-right">
														@string.Format("{0:c}", @item.PerDiem_Amount)

													</td>
												</tr>
											}


										</tbody>
									</table>
								</div>
							</div>

							<div class="col-md-12 form-group">
								<h2 class="heading-one mb-0">B- Per Diem</h2>

								<div class="table-responsive">
									<table id="tblvoucher" class="table v-top table-stripted lg-table mb-0 mt-0">
										<thead>
											<tr>
												<th width="10">SNo. </th>
												<th width="90">Date</th>
												<th>Name of City</th>
												<th>Class of City</th>
												<th>Project Name</th>
												<th width="110">Free Meal/Others</th>
												<th width="90">Per Diem Rate</th>
												<th width="110"><i class="fas fa-rupee-sign "></i>Amount (Rs.)</th>

											</tr>
										</thead>
										<tbody>
											@{ int countdiem = 0;}
											@foreach (var item in Model.BPERDIEM)
											{
												countdiem++;
												DateTime dateTime = Convert.ToDateTime(item.Date);
												<tr>
													<td>@countdiem</td>


													<td>@dateTime.ToString("dd/MM/yyyy")</td>


													<td>@item.CityName</td>


													<td>@item.ClassofCity</td>


													<td>@item.ProjectName</td>

													@{ var freemeal = Model.FreemealList.Where(x => x.ID == item.FreeMealID).Select(x => x.Percantage).FirstOrDefault();
														var freemealname = Model.FreemealList.Where(x => x.ID == item.FreeMealID).Select(x => x.Name).FirstOrDefault();
													}
													<td class="text-right">@freemealname</td>


													<td class="text-right">@item.PerDiemRate</td>
													<td class="text-right">
														@string.Format("{0:c}", @item.Amount)

													</td>
												</tr>
											}


										</tbody>
									</table>
								</div>
							</div>


							<div class="col-md-12 form-group">
								<h2 class="heading-one mb-0">C-Transportation</h2>

								<div class="table-responsive">
									<table id="tblvoucher" class="table v-top table-stripted lg-table mb-0 mt-0">
										<thead>
											<tr>
												<th width="10">SNo. </th>
												<th>Project</th>
												<th width="90">Date</th>
												<th width="150">Mode of Transport</th>
												<th>Details</th>
												<th width="90">Attachment No</th>
												<th width="110">Travel KM</th>
												<th width="90">KM</th>
												<th width="110"><i class="fas fa-rupee-sign "></i>Amount (Rs.)</th>

											</tr>
										</thead>
										<tbody>
											@{ int counttrans = 0;}
											@foreach (var item in Model.CTRANSPORTATION)
											{
												DateTime dateTime = Convert.ToDateTime(item.Date);
												counttrans++;
												<tr>
													<td>@counttrans</td>
													<td>@item.ProjectName</td>
													<td>@dateTime.ToString("dd/MM/yyyy")</td>
													<td>
														@if (item.ModeOfTransport != "")
														{
															@item.ModeOfTransport
														}
														else
														{
															@item.ModeofTrnas
														}
													</td>
													<td>@item.Details</td>
													<td class="text-right">@item.AttachmentNo</td>
													<td class="text-right">@item.RateofPerKM</td>
													<td class="text-right">@item.TravelKM</td>

													<td class="text-right">
														@string.Format("{0:c}", @item.Amount)

													</td>
												</tr>

											}


										</tbody>
									</table>
								</div>
							</div>

							<div class="col-md-12 form-group">
								<h2 class="heading-one mb-0">D- Other Expenditure</h2>

								<div class="table-responsive">
									<table id="tblvoucher" class="table v-top table-stripted lg-table mb-0 mt-0">
										<thead>
											<tr>
												<th width="10">SNo. </th>
												<th width="150">Project</th>
												<th width="90">Date</th>
												<th>Details	</th>
												<th width="90">Attachment No</th>
												<th width="110"><i class="fas fa-rupee-sign "></i>Amount (Rs.)</th>

											</tr>
										</thead>
										<tbody>
											@{ int countexp = 0;}
											@foreach (var item in Model.DOTHEREXPENDITURE)
											{
												countexp++;
												DateTime dateTime = Convert.ToDateTime(item.Date);
												<tr>
													<td>@countexp</td>
													<td>@item.ProjectName</td>
													<td>@dateTime.ToString("dd/MM/yyyy")</td>
													<td>@item.DetailsofExpenditure	</td>
													<td class="text-right">@item.AttachmentNo</td>

													<td class="text-right">@string.Format("{0:c}", @item.Amount)</td>
												</tr>
											}



										</tbody>
									</table>
								</div>
							</div>

							<div class="col-md-12 form-group">
								<h2 class="heading-one mb-0">E - Other Credit</h2>

								<div class="table-responsive">
									<table id="tblvoucher" class="table v-top table-stripted lg-table mb-0 mt-0">
										<thead>
											<tr>
												<th>Details of Credit</th>
												<th width="110"><i class="fas fa-rupee-sign "></i>Amount (Rs.)</th>
											</tr>
										</thead>
										<tbody>
											<tr>
												<td>@Model.TravelExpense.credit_details </td>
												<td class="text-right">
													@string.Format("{0:c}", @Model.TravelExpense.amount)

												</td>

											</tr>
										</tbody>
									</table>
								</div>

								<div class="mt-2">
									<label>Remarks</label>
									<p>@Model.TravelExpense.remark</p>
								</div>


							</div>

							<div class="col-md-12 form-group">
								<h2 class="heading-one mb-0">F - Trip Report</h2>
								<table class="table v-top table-stripted lg-table mb-0 mt-0">
									<thead>
										<tr>
											<th>S.No.</th>
											<th>Date </th>
											<th>Description </th>
										</tr>
									</thead>
									<tbody>
										@if (Model.TripReport != null)
										{
											int CountTripReport = 0;
											for (int i = 0; i < Model.TripReport.Count; i++)
											{
												CountTripReport++;
												DateTime dateTime = Convert.ToDateTime(Model.TripReport[i].Date);


												if (Model.TripReport[i].Justification != null)
												{
													<tr>
														<td>
															@CountTripReport
														</td>
														<td>

															@dateTime.ToString("dd/MM/yyyy")

														</td>
														<td>

															<p>
																@Model.TripReport[i].Justification
															</p>
															@*<i class="fas fa-info-circle td-info" data-toggle="tooltip" data-placement="right" data-original-title="@Model.TripReport[i].Justification"></i>*@

														</td>


													</tr>
												}

											}
										}
									</tbody>
								</table>

							</div>
							<div class="col-md-12 form-group">
								<h2 class="heading-one mb-0">Attachment</h2>
								@{Html.RenderAction("_ViewAllAttachments", "Master", new { src = clsApplicationSetting.EncryptQueryString(ViewBag.MenuID.ToString() + "*/Master/_ViewAllAttachments*" + Model.TravelExpense.ReqNo + "*TravelExpense") });}
							</div>
								
								@*<div class="col-md-12 form-group">
				<h2 class="heading-one mb-0">Attachment</h2>
				<table class="table v-top table-stripted lg-table mb-0 mt-0">
					  <thead>
						<tr>
						  <th width="33">S.No</th>
						  <th width="300">Attachment</th>
						  <th>Description</th>

						</tr>
					  </thead>
					  <tbody>
						<tr>
						  <td>1</td>
						  <td><a href="/Attachments/8104.docx"><strong class="green-clr">DL_Demo.docx</strong></a></td>
						  <td></td>
						</tr>
					  </tbody>
					</table>

			</div>*@
								<div class="col-md-12 form-group">
									<label>Payment Mode</label>
									<p>@Model.TravelExpense.pay_mode</p>
								</div>

								<div class="col-md-12 form-group">
									<h2 class="heading-one mb-0">Do you want to distrubute expense equaly?</h2>
									<p>@Model.TravelExpense.Expensedistrubute_status</p>
								</div>



								<div class="col-md-12 form-group">
									<h2 class="heading-one mb-0">Summary</h2>

									<div class="table-responsive">
										<table id="tblvoucher" class="table v-top table-stripted lg-table mb-0 mt-0">
											<thead>
												<tr>
													<th>Particular</th>
													@foreach (var item in Model.ProjectList)
													{
														<th class="text-right tw-100">@item.Proj_name</th>
													}


													<th width="80">Total</th>
												</tr>
											</thead>
											<tbody>
												<tr>
													<td>Travel Fare</td>
													@for (int i = 0; i < Model.EXPENSESUMMARY.Count; i++)
													{
														<td class="text-right">
															@string.Format("{0:c}", @Model.EXPENSESUMMARY[i].Travelfare)
															@Html.HiddenFor(model => model.EXPENSESUMMARY[i].Travelfare, new { @class = "form-control inp-none text-right  EAmount", @readonly = "readonly" })
														</td>
													}

													<td class="text-right">
														@string.Format("{0:c}", @Model.TravelExpense.travel_fare)
														@Html.HiddenFor(model => model.TravelExpense.travel_fare, new { @class = "form-control text-right inp-none totalA", @readonly = "readonly" })

													</td>
												</tr>
												<tr>
													<td>Per Diem</td>
													@for (int i = 0; i < Model.EXPENSESUMMARY.Count; i++)
													{
														<td class="text-right">
															@string.Format("{0:c}", @Model.EXPENSESUMMARY[i].Perdiem)
															@Html.HiddenFor(model => model.EXPENSESUMMARY[i].Perdiem, new { @class = "form-control inp-none dvdperdiemrate text-right FAmount", @readonly = "readonly" })
														</td>

													}

													<td class="text-right">
														@string.Format("{0:c}", @Model.TravelExpense.per_Diem)
														@Html.HiddenFor(model => model.TravelExpense.per_Diem, new { @class = "form-control text-right  inp-none totalB", @readonly = "readonly" })

													</td>
												</tr>

												<tr>
													<td>Transportation</td>
													@for (int i = 0; i < Model.EXPENSESUMMARY.Count; i++)
													{
														<td class="text-right">
															@string.Format("{0:c}", @Model.EXPENSESUMMARY[i].Transportion)
															@Html.HiddenFor(model => model.EXPENSESUMMARY[i].Transportion, new { @class = "form-control inp-none dvdtransportion text-right GAmount", @readonly = "readonly" })
														</td>

													}

													<td class="text-right">
														@string.Format("{0:c}", @Model.TravelExpense.transporation)
														@Html.HiddenFor(model => model.TravelExpense.transporation, new { @class = "form-control inp-none totalC text-right", @readonly = "readonly" })

													</td>
												</tr>

												<tr>
													<td>Other Expenses</td>
													@for (int i = 0; i < Model.EXPENSESUMMARY.Count; i++)
													{
														<td class="text-right">
															@string.Format("{0:c}", @Model.EXPENSESUMMARY[i].Otherexpesnse)
															@Html.HiddenFor(model => model.EXPENSESUMMARY[i].Otherexpesnse, new { @class = "form-control inp-none dvdExpenses text-right HAmount", @readonly = "readonly" })
														</td>

													}

													<td class="text-right">
														@string.Format("{0:c}", @Model.TravelExpense.other_expense)
														@Html.HiddenFor(model => model.TravelExpense.other_expense, new { @class = "form-control inp-none text-right totalD", @readonly = "readonly" })
													</td>
												</tr>
												<tr>

													<td>Total</td>
													@*<td class="text-right">1130</td>
								<td class="text-right">750</td>
								<td class="text-right">500</td>
								<td class="text-right">2380</td>*@
													@if (Model.ProjectList != null)
													{
														int counttotal = 0;
														foreach (var item in Model.ProjectList)
														{
															counttotal++;

															<td class="text-right prntTotal"><input type="text" id="txttotal_@counttotal" class="dvdTotal form-control text-right inp-none" readonly /></td>
														}
													}
													<td class="text-right">

														@Html.TextBoxFor(model => model.TravelExpense.total, new { @class = "form-control inp-none totalFinal text-right", @readonly = "readonly", Value = string.Format("{0:c}", @Model.TravelExpense.total) })

													</td>
												</tr>
												<tr>
													<td>Less: Advance Received</td>
													@foreach (var item in Model.ProjectList)
													{

														<td class="text-right"></td>
													}
													<td class="text-right">
														@string.Format("{0:c}", @Model.TravelExpense.Paidamount)

													</td>
												</tr>

												<tr>
													<td>Less: Any Other Credits</td>
													@foreach (var item in Model.ProjectList)
													{

														<td class="text-right"></td>
													}
													<td class="text-right">
														@string.Format("{0:c}", @Model.TravelExpense.anyOther_Credit)

													</td>
												</tr>
											</tbody>
											<tfoot>
