													int AKPCount = 0;
													foreach (var item in Model.KPAL)
													{
														AKPCount++;
														<li class="nav-item">
															<a class="nav-link @(AKPCount == 1 ? "active" : "")" id="home-tab" data-toggle="tab" href="#tab_@(item.PMS_ADID)" role="tab" aria-controls="home" aria-selected="true">
