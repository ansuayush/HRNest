@model List<LockUnlock.HistoryList>


@if (Model.Count > 0)
{
	<ul class="list-unstyled cmtlist scroll p-2 mb-2">
		@foreach (var item in Model)
		{
			<li>
				<strong>@item.Name</strong>, <span class="lgt-clr">@item.Date</span>
				<div class="cmt-box">
					@item.Remarks
				</div>
			</li>
		}

	</ul>
}
