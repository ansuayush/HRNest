<div class="row">
	<div class="col-sm-5"><h1 class="heading-one ">@Model</h1></div>
	<div class="col-sm-7 text-right">
		@{
			string[] Requestedsrc = null;
			Requestedsrc = clsApplicationSetting.DecryptQueryString(HttpContext.Current.Request.QueryString["src"]);
		}
		@if (Requestedsrc != null)
		{
			if (Requestedsrc.Length > 3 && Requestedsrc[3] != "")
			{
				if (Requestedsrc[3] == "Mobile")
				{
					<a id="AnchorAddButton" class="btn mtr-g-grn btn-l ripplelink " onclick="ListHeaderAdd();">
						<i class="fa fa-plus"></i><span class="sbAtext"></span>
					</a>
				}

			}
			@*else
			{
				<a id="AnchorAddButton" class="btn mtr-g-grn btn-l ripplelink " onclick="ListHeaderAdd();">
					<i class="fa fa-plus"></i> <span class="sbAtext">Add</span>
				</a>
			}*@
		}
		@if (clsApplicationSetting.GetSessionValue("Write") == "True")
		{
			<a id="AnchorAddButton" class="btn mtr-g-grn btn-l ripplelink " onclick="ListHeaderAdd();">
				<i class="fa fa-plus"></i> <span class="sbAtext">Add</span>
			</a>
		}
		@if (clsApplicationSetting.GetSessionValue("Export") == "True")
		{
			<a id="anchListExcelReport" onclick="ListHeaderExport();" class="btn blk-gdn-btn btn-l ripplelink">
				<i class="fa fa-file-excel"></i> Export
			</a>
		}

	
	</div>

</div>





<script type="text/javascript">

    function ListHeaderAdd() {
        var count = 0;
		ShowLoadingDialog();
		if ($("#hdnAddUrl").val()) {
			window.location = $("#hdnAddUrl").val();
		}
		else {
			CloseLoadingDialog();
			FailToaster("Link not found");
		}
    }

    function ListHeaderExport() {
		var count = 0;
		if ($("#hdnExportUrl").val()) {
			window.location = $("#hdnExportUrl").val();
		}
		else {
			CloseLoadingDialog();
			FailToaster("Link not found");
		}
    }



</script>
