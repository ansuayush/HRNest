@{
	int TabIndex = 0;
	long EMPID = 0;
	long UserId = 0;
	long LocationId = 0;
	long CandidateID = 0;
	long LoginID = 0;
	long.TryParse(ViewBag.EMPID, out EMPID);
	long.TryParse(ViewBag.CandidateID, out CandidateID);
	long.TryParse(ViewBag.LoginID, out LoginID);
	long.TryParse(ViewBag.LocationId, out LocationId);
	if (ViewBag.Mobile == "Mobile")
	{
		long.TryParse(ViewBag.UserId, out UserId);
	}

	if (ViewBag.TabIndex != null)
	{
		TabIndex = ViewBag.TabIndex;
	}
}
<div class="empmenu navsky">
<a class="logo" id ="achorname" href="">My Profile</a>
    <input id="nav" type="checkbox">
    <label for="nav"></label>
    <nav>
        <ul class="navbar-scd list-unstyled ">
            <li class="@(TabIndex==1?"active":"")">

                @if (ViewBag.Mobile == "Mobile")
                {
                    <a onclick="ShowLoadingDialog();window.location='@Url.Action("RegistrationDetails",new { src = clsApplicationSetting.EncryptQueryString(ViewBag.MenuID.ToString() + "*/OnboardingEmployee/RegistrationDetails" + "*" + CandidateID + "*" +EMPID + "*" + LoginID) })';">
                        <span class="btn btn-default btn-circle">1</span> Registration <span class="d-block">Details</span>
                    </a>
                }
                else
                {
                    <a onclick="ShowLoadingDialog();window.location='@Url.Action("RegistrationDetails",new { src = clsApplicationSetting.EncryptQueryString(ViewBag.MenuID.ToString() + "*/OnboardingEmployee/RegistrationDetails" + "*" + CandidateID + "*" + EMPID + "*" + LoginID) })';">
                        <span class="btn btn-default btn-circle">1</span> Registration <span class="d-block">Details</span>
                    </a>
                }

            </li>
            @if (EMPID > 0)
            {
                if (ViewBag.Mobile == "Mobile")
                {
                    <li class="@(TabIndex==2?"active":"")">
