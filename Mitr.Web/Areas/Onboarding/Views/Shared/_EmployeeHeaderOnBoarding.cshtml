                        </a>
                        @*<a href="#">
                        <span class="btn btn-default btn-circle">5</span> Declaration
                    </a>*@
                    </li>
                }
                else
                {
                    <li class="@(TabIndex==2?"active":"")">
                        <a onclick="ShowLoadingDialog();window.location='@Url.Action("PersonalDetails",new { src = clsApplicationSetting.EncryptQueryString(ViewBag.MenuID.ToString()+"*/OnboardingEmployee/PersonalDetails"+ "*" + CandidateID + "*" +EMPID + "*" + LoginID) })';">
