        </script>

        <script>
            $(document).ready(function () {
                // When the 'Check All' checkbox is clicked
                $('#checkAllone').click(function () {
                    // This will check or uncheck all checkboxes depending on the 'Check All' checkbox's state
                    $('.itemCheckboxone').prop('checked', this.checked);
                });

                // When any single checkbox is unchecked, uncheck the 'Check All' checkbox
                // If all are checked, check the 'Check All' checkbox
                $('.itemCheckboxone').change(function () {
                    var allChecked = $('.itemCheckboxone').length === $('.itemCheckboxone:checked').length;
                    $('#checkAllone').prop('checked', allChecked);
                });
            });


            $('.subtask').change(function () {
                $('.subtask_yes').hide();
                $('#responseType-1').removeClass('Mandatory');
                $('#subTaskName-1').removeClass('Mandatory');
                $('#spresponseType-1').hide();
                $('#spsubTaskName-1').hide();
                $('#responseType-1').val('');
                $('#subTaskName-1').val('');
                if ($(this).val() == 'subtask_yes') {
                    $('#responseType-1').addClass('Mandatory');
                    $('#subTaskName-1').addClass('Mandatory');
                    $('.' + $(this).val()).show();
                }

            });


            $('.erselect').on("change", function () {
                $('.ercontent').hide();
                $('#EscalateTo').removeClass('Mandatory');
                $('#EscalationTime').removeClass('Mandatory');
                $('#ReminderTime').removeClass('Mandatory');
                $('#spEscalateTo').hide();
                $('#spEscalationTime').hide();
                $('#spReminderTime').hide();
                $("#EscalateTo").val('').trigger('change');
                $('#EscalationTime').val('');
                $('#ReminderTime').val('');
                if ($(this).val() == 'er_yes') {
                    $('#EscalateTo').addClass('Mandatory');
                    $('#EscalationTime').addClass('Mandatory');
                    $('#ReminderTime').addClass('Mandatory');
                }
                $('.' + $(this).val()).show();
            });


            $('#ddlCompType').on("change", function () {
                if ($(this).val() == 1) {
                    $('#ddlFrequency').prop('disabled', true);
                    $('#ddlFrequency').removeClass('Mandatory');
                    $('#dayofM').removeClass('Mandatory');
                    $('#dayofY').removeClass('Mandatory');
                }
                else {
                    $('#ddlFrequency').prop('disabled', false);
                    $('#ddlFrequency').addClass('Mandatory');
                    $('#dayofM').addClass('Mandatory');
                    $('#dayofY').addClass('Mandatory');
                }

            });





        </script>







