        <script>
            $('.selectvaul').change(function () { //on change do stuff
                $('.box').hide(); //hide all with .box class
                if ($(this).val() == 'weekly_1') {
                    $('#spWeekly').hide();
                    $('#dayofM').val('');
                    $('#dayofM').removeClass('Mandatory');
                    $('input[name="checkbox"]').prop('checked', false);
                    $('#dayofY').val('');
                    $('#dayofY').removeClass('Mandatory');
                    $('input[name="year"]').prop('checked', false);

                }
                else if ($(this).val() == 'months_2') {
                    $('#spdayofM').hide();
                    $('#dayofM').addClass('Mandatory');
                    $('input[name="weekly"]').prop('checked', false);
                    $('#dayofY').val('');
                    $('#dayofY').removeClass('Mandatory');
                    $('input[name="year"]').prop('checked', false);
                    $('#dayofY').removeClass('Mandatory');

                }
                else if ($(this).val() == 'yearly_3') {
                    $('#spdayofY').hide();
                    $('#dayofY').addClass('Mandatory');
                    $('#dayofM').val('');
                    $('#dayofM').removeClass('Mandatory');
                    $('input[name="checkbox"]').prop('checked', false);
                    $('input[name="weekly"]').prop('checked', false);
                }
                else {
                    $('#dayofM').val('');
                    $('#dayofM').removeClass('Mandatory');
                    $('input[name="checkbox"]').prop('checked', false);
                    $('input[name="weekly"]').prop('checked', false);
                    $('#dayofY').val('');
                    $('#dayofY').removeClass('Mandatory');
                    $('input[name="year"]').prop('checked', false);
                }
                $('.' + $(this).val()).show(); //show selected option's respective element
            });
        </script>
