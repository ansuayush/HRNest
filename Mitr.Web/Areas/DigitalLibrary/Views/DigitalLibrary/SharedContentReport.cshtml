
@{
    ViewBag.Title = "Report";
}
<style>

    .dx-datagrid {
        font-size: 12px;
    }

        .dx-datagrid .dx-datagrid-headers {
            font-size: 25px;
        }

        .dx-datagrid .dx-row {
            font-size: 12px;
            word-wrap: break-word;
        }

    .adaptiveRowStyle {
        font-size: 20pt;
        word-wrap: break-word;
    }
</style>
<div class="inner-main defult-height pb-0" data-select2-id="10">
    <h1 class="heading-one ">Documents Shared by Team members</h1>

    <div class="row" data-select2-id="9">

        <div class="col-lg-2 col-md-2 col-sm-2  form-group">
            <label class="lbl-abs">From <sup>*</sup></label>
            <input class="form-control actdate" id="StartDate" name="StartDate" type="date" value=""   onchange="HideErrorMessage(this)">
            <span id="spStartDate" class="text-danger field-validation-error" style="display:none;">Hey, You missed this field!!</span>
        </div>
        <div class="col-lg-2 col-md-2 col-sm-2  form-group">
            <label class="lbl-abs">To</label>
            <input class="form-control actdate ddapplyaction" id="EndDate" name="EndDate" type="date" value="" onchange="HideErrorMessage(this)">
            <span id="spEndDate" class="text-danger field-validation-error" style="display:none;">Hey, You missed this field!!</span>

        </div>

        <div class="col-lg-2 col-md-2 col-sm-2 slt-dgn  form-group" data-select2-id="8">
            <label class="lbl-abs">Category</label>
            <select class="form-control applyselect select2-hidden-accessible" id="ddlCategory" data-select2-id="1" tabindex="-1" aria-hidden="true">
            </select>

        </div>



        <div class="col-lg-2 col-md-2 col-sm-2 slt-dgn  form-group" data-select2-id="28">
            <label class="lbl-abs">Documents Sensitivity</label>
            <select class="form-control applyselect select2-hidden-accessible" id="ddlDocCategory" data-select2-id="4" tabindex="-1" aria-hidden="true">
                <option data-select2-id="0">Select</option>
                <option data-select2-id="Critical and Confidential">Critical and Confidential</option>
                <option data-select2-id="Critical and Not Confidential">Critical and Not Confidential</option>
                <option data-select2-id="Not Critical and Not Confidential">Not Critical and Not Confidential</option>

            </select>
        </div>


        <div class="align-self-center">
            <button type="button" onclick="Search()" class="btn mtr-g-grn btn-l ripplelink" id="btnSearch" name="Command" value="Search" fdprocessedid="axo3nq"><i class="fas fa-search"></i>Search</button>
            <button type="button" onclick="Export()" class="btn mtr-g-grn btn-l ripplelink" id="btnExportAll" value="Search" fdprocessedid="34x0kq"><i class="fas fa-file-excel"></i>Export</button>
        </div>

        <div class="col-sm-12">


            <div class="table-responsive">
                <table id="table" class="table table-striped table-bordered dt-responsive nowrap tbltick new_width" style="border-collapse: collapse; border-spacing: 0; width: 100%;">
                    <thead>
                        <tr>

                            <th width="90">Date</th>
                            <th>Sender</th>
                            <th>Receiver</th>
                            <th width="250"> Type of Document(Attachment Type)</th>
                            <th width="150">Category</th>
                            <th width="150">Subcategory</th>
                            <th width="150">Documents Sensitivity</th>

                        </tr>
                    </thead>
                </table>
            </div>
        </div>

    </div>
</div>


