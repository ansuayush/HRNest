
@{
    ViewBag.Title = "ModuleAdmin";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<!-- Start -->
<div class="inner-main defult-height p-3 pb-0">
    <h1 class="heading-one">Module Admin</h1>

    <div class="row">

        <div class="col-sm-12">
            <div class="tab-main mb-3">

                <ul id="tabs" class="nav nav-tabs " role="tablist">
                    <li class="nav-item"> <a id="tab-A" href="#pending" class="nav-link active ripplelink" data-toggle="tab" role="tab"><i class="fas fa-clock"></i>Pending</a> </li>
                    <li class="nav-item"> <a id="tab-B" href="#underprocessed" class="nav-link ripplelink" data-toggle="tab" role="tab"><i class="fas fa-cogs"></i>Process</a> </li>
                    <li class="nav-item"> <a id="tab-B" href="#Rejected" class="nav-link ripplelink" data-toggle="tab" role="tab"><i class="fas fa-cogs"></i>Reject</a> </li>
                    <li class="nav-item">
                        <a id="tab-D" href="#live" class="nav-link ripplelink" data-toggle="tab" role="tab">
                            <i class="fas fa-database"></i>Live
                        </a>
                    </li>

                </ul>

                <div id="content" class="tab-content p-0" role="tablist">
                    <!-- --->
                    <div id="pending" class="card tab-pane fade show   active" role="tabpanel" aria-labelledby="tab-A">
                        <div class="card-header" role="tab" id="heading-A">
                            <h5 class="mb-0"> <a data-toggle="collapse" href="#collapse-A" aria-expanded="true" aria-controls="collapse-A">Pending <i class="fas fa-clock"></i></a> </h5>
                        </div>
                        <div id="collapse-A" class="collapse show" data-parent="#content" role="tabpanel" aria-labelledby="heading-A">
                            <div class="table-responsive">
                                <table class="table mt-4" id="tblPending">
                                    <thead>
                                        <tr>
                                            <th width="10">S.No</th>
                                            <th width="80">Request No</th>
                                            <th width="100">Request Date</th>
                                            <th width="150">Process By</th>
                                            <th width="150">Point of Contact</th>
                                            <th>Project Code</th>
                                            <th>Contract/Agreement Type </th>
                                            <th width="100">Req. Amount</th>
                                            <th width="140">Status</th>
                                            <th width="70">Action</th>
                                        </tr>
                                    </thead>

                                </table>
                            </div>
                        </div>
                    </div>

                    <div id="underprocessed" class="card tab-pane fade " role="tabpanel" aria-labelledby="tab-b">
                        <div class="card-header" role="tab" id="heading-A">
                            <h5 class="mb-0"> <a data-toggle="collapse" href="#collapse-b" aria-expanded="true" aria-controls="collapse-b">Under Processed <i class="fas fa-cog"></i></a> </h5>
                        </div>
                        <div id="collapse-b" class="collapse " data-parent="#content" role="tabpanel" aria-labelledby="heading-b">
                            <div class="table-responsive">
                                <table class="table mt-4" id="tblUnderprocessed">
                                    <thead>
                                        <tr>
                                            <th width="10">S.No</th>
                                            <th width="80">Request No</th>
                                            <th width="100">Request Date</th>
                                            <th width="150">Process By</th>
                                            <th width="150">Point of Contact</th>
                                            <th>Project Code</th>
                                            <th>Contract/Agreement Type </th>
                                            <th width="100">Req. Amount</th>
                                            <th width="140">Status</th>
                                            <th width="70">Action</th>
                                        </tr>
                                    </thead>

                                </table>
                            </div>
                        </div>
                    </div>

                    <div id="Rejected" class="card tab-pane fade " role="tabpanel" aria-labelledby="tab-b">
                        <div class="card-header" role="tab" id="heading-A">
                            <h5 class="mb-0"> <a data-toggle="collapse" href="#collapse-b" aria-expanded="true" aria-controls="collapse-b">Rejected <i class="fas fa-cog"></i></a> </h5>
                        </div>
                        <div id="collapse-b" class="collapse " data-parent="#content" role="tabpanel" aria-labelledby="heading-b">
                            <div class="table-responsive">
                                <table class="table mt-4" id="tblRejected">
                                    <thead>
                                        <tr>
                                            <th width="10">S.No</th>
                                            <th width="80">Request No</th>
                                            <th width="100">Request Date</th>
                                            <th width="150">Process By</th>
                                            <th width="150">Point of Contact</th>
                                            <th>Project Code</th>
                                            <th>Contract/Agreement Type </th>
                                            <th width="100">Req. Amount</th>
                                            <th width="140">Status</th>
                                            <th width="70">Action</th>
                                        </tr>
                                    </thead>

                                </table>
                            </div>
                        </div>
                    </div>
                    <div id="live" class="card tab-pane fade " role="tabpanel" aria-labelledby="tab-D">
                        <div class="card-header" role="tab" id="heading-D">
                            <h5 class="mb-0"> <a data-toggle="collapse" href="#collapse-D" aria-expanded="true" aria-controls="collapse-D">Live <i class="fas fa-database"></i></a> </h5>
                        </div>
                        <div id="collapse-D" class="collapse " data-parent="#content" role="tabpanel" aria-labelledby="heading-c">
                            <div class="table-responsive">
                                <table class="table mt-4" id="tbllive">
                                    <thead>
                                        <tr>
                                            <th width="10">S.No</th>
                                            <th width="80">Request No</th>
                                            <th width="100">Request Date</th>
                                            <th width="150">Process By</th>
                                            <th width="150">Point of Contact</th>
                                            <th>Project Code</th>
                                            <th>Contract/Agreement Type </th>
                                            <th width="100">Req. Amount</th>
                                            <th width="140">Status</th>
                                            <th width="70">Action</th>
                                        </tr>
                                    </thead>

                                </table>
                            </div>

                        </div>
                    </div>




                </div>

            </div>
        </div>
    </div>
</div>
<!-- ENd -->

@section scripts    {
    @System.Web.Optimization.Scripts.Render("~/bundle/unobtrusivejs")
    @System.Web.Optimization.Scripts.Render("~/bundle/dataTablesjs")
    <script src="~/assets/design/Scripts/AddInit.js?v=@clsApplicationSetting.GetWebConfigValue("Version")"></script> 
    <script src="~/Areas/Procurement/DevScript/ModuleAdmin.js"></script>
    <script>
		 var IsEDUser = '@ViewBag.IsED';
       var loggedinUserid = '@ViewBag.UserId';
		var loggedinUserName = '@ViewBag.UserName';
		var UserGrade = '@ViewBag.Grade';
        var IsPM = '@ViewBag.IsPM';



    </script>
}

