<script>
    function ChangeAmendmentDetails(ctrl) {
        var Id = ctrl.value;

        var Status = $('#ddlAmendmentVersion option[value="' + $(ctrl).val() + '"]').attr("dataEle")
        var statudData = Status.split('-');
        var StatusCode = statudData[0];
        var ContractType = statudData[1];
        var DataFrom = statudData[2];
        var Parent = statudData[3];
        if (ContractType == "1") {
            ContractType = "Consultant";
        }
        else if (ContractType == "2") {
            ContractType = "Sub-grant";

        }
        else {
            ContractType = "Purchase Order (PO)";
        }

        RedirectToData(Id, StatusCode, ContractType, DataFrom, Parent);
    }

    function RedirectToData(Id, StatusCode, ContractType, DataFrom, Parent) {
        //12	 RFP Is Live
        //15	Waiver request approved	ED Approved Qutation Entry
        //17	RFP No for Quotation RFP No
        //18	Draft saved	Quotation Entry Save by user
        //19	Pending with PC	Quotation Entry Submit by user
        //20	Approved by PC	Quotation Entry approved by Commetee member
        //21	Approval in Process	Aproved by one
        //24	With Authorised Signatory	Quotation Entry fwd by module admin to auth sign
        //25	Approved by Authorised Signatory	Auth sign approved
        //27	Draft saved	Module Admin Contract sign Save

        if (ContractType == 'Consultant') {
            RedirectToQuotation(Id, DataFrom, 1);
        }
        else if (ContractType == 'Sub-grant') {
            RedirectToQuotation(Id, DataFrom, 2);
        }
        else {
            RedirectToQuotation(Id, DataFrom, 3);

        }



    }

    function RedirectToQuotation(id, datafrom, type) {
        var url = "";
        if (type == 1) {
            if (datafrom == 1) {
                url = "/Procurement/FinanceQuotationConsultantRFP?id=" + id;
                window.location.href = url;
            }
            else if (datafrom == 2) {
                url = "/Procurement/FinanceQuotationConsultant?id=" + id;
                window.location.href = url;
            }
        }
        else if (type == 2) {
            if (datafrom == 1) {
                url = "/Procurement/QuotationEntryPendingSubgrantRFP?id=" + id;
                window.location.href = url;

            }
            else if (datafrom == 2) {
                url = "/Procurement/QuotationEntryPendingSubgrant?id=" + id;
                window.location.href = url;
            }
        }
        else {
            if (datafrom == 1) {
                url = "/Procurement/FinanceQuotationRFP?id=" + id;
                window.location.href = url;
            }
            else if (datafrom == 2) {
                url = "/Procurement/FinanceQuotation?id=" + id;
                window.location.href = url;
            }

        }
    }

